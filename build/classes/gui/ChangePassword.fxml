<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="293.0" prefWidth="446.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.ChangePasswordController">
   <children>
      <Label layoutX="82.0" text="Change Password">
         <font>
            <Font size="36.0" />
         </font></Label>
      <Button fx:id="btn_save" layoutX="109.0" layoutY="231.0" mnemonicParsing="false" onAction="#_save" prefHeight="39.0" prefWidth="104.0" text="Save">
         <font>
            <Font size="18.0" />
         </font></Button>
      <JFXPasswordField fx:id="tf_passwordConfirm" layoutX="10.0" layoutY="171.0" prefHeight="25.0" prefWidth="314.0" promptText="Confirm Password" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0">
